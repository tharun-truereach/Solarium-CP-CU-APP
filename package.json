{
  "name": "solarium-cp-cu-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:changed": "vitest related --run",
    "test:a11y": "vitest run --config vitest.a11y.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:security": "eslint . --ext ts,tsx --config .eslintrc.security.cjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "qa:check": "npm run lint && npm run type-check && npm run test:coverage && npm run test:a11y",
    "qa:fix": "npm run lint:fix && npm run format && npm run test:coverage",
    "coverage:badges": "node scripts/generate-badges.js",
    "coverage:open": "open coverage/index.html",
    "security:audit": "npm audit --audit-level=moderate",
    "security:scan": "npm run lint:security && npm run security:audit",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist --open",
    "clean": "rm -rf dist coverage node_modules/.vite",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/crypto-js": "^4.2.1",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "axe-core": "^4.8.2",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.3.2",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}