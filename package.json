{
  "name": "solarium-cp-cu-app",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "description": "Comprehensive lead management platform for solar product sales",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:threshold": "vitest --coverage --coverage.threshold.statements=85 --coverage.threshold.branches=80 --coverage.threshold.functions=85 --coverage.threshold.lines=85",
    "test:watch": "vitest --watch",
    "test:changed": "vitest related --run",
    "test:a11y": "vitest run --config vitest.a11y.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:leads": "vitest run src/api/endpoints/__tests__/leadEndpoints.test.tsx src/utils/__tests__/leadAccessGuard.test.ts src/hooks/__tests__/useLeadAccess.test.tsx",
    "test:coverage:leads": "vitest --coverage src/api/endpoints/leadEndpoints.ts src/utils/leadAccessGuard.ts src/hooks/useLeadAccess.ts",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:headless": "cypress run --headless",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:mobile": "cypress run --spec 'cypress/e2e/*mobile*.spec.ts'",
    "cy:run:performance": "cypress run --spec 'cypress/e2e/leads.spec.ts' --env performance=true",
    "test:e2e:cypress": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cy:run:headless",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:security": "eslint . --ext ts,tsx --config .eslintrc.security.cjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "qa:check": "npm run lint && npm run type-check && npm run test:coverage && npm run test:a11y",
    "quality-check": "npm run lint && npm run type-check && npm run test:coverage-report && npm run test:security",
    "qa:fix": "npm run lint:fix && npm run format && npm run test:coverage",
    "coverage:badges": "node scripts/generate-badges.js",
    "coverage:open": "open coverage/index.html",
    "security:audit": "npm audit --audit-level=moderate",
    "security:scan": "npm run lint:security && npm run security:audit",
    "test:coverage-report": "npm run test:coverage && node scripts/coverage-report.js",
    "test:security": "npm run lint:security && npm run test tests/security/",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist --open",
    "clean": "rm -rf dist coverage node_modules/.vite",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/x-date-pickers": "^8.5.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/crypto-js": "^4.2.1",
    "@uiw/react-json-view": "^2.0.0-alpha.32",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.48.2",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-window": "^1.8.11",
    "redux-persist": "^6.0.0",
    "use-immer": "^0.9.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.9.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "axe-core": "^4.8.2",
    "cypress": "^13.17.0",
    "cypress-audit": "^1.1.0",
    "cypress-axe": "^1.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.3.5",
    "prettier": "^3.1.0",
    "start-server-and-test": "^2.0.12",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "react",
    "typescript",
    "solar",
    "lead-management",
    "crm",
    "settings-management"
  ],
  "author": "Solarium Development Team",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
