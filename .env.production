# Production Environment Configuration
# Note: All variables must start with REACT_APP_ to be accessible in the browser

# Environment identifier
REACT_APP_ENVIRONMENT=PROD

# API Configuration
REACT_APP_API_BASE_URL=https://api.solarium.com
REACT_APP_API_TIMEOUT=30000

# Session Configuration (in minutes)
REACT_APP_SESSION_TIMEOUT_MIN=30
REACT_APP_SESSION_WARNING_MIN=5

# Feature Flags
REACT_APP_ENABLE_DEBUG_TOOLS=false
REACT_APP_ENABLE_MOCK_AUTH=false
REACT_APP_ENABLE_SERVICE_WORKER=true

# External Services
REACT_APP_SENTRY_DSN=https://production-sentry-dsn@sentry.io/project
REACT_APP_ANALYTICS_ID=UA-PRODUCTION-ID

# Build Configuration
REACT_APP_BUILD_NUMBER=${BUILD_NUMBER:-prod-build}
REACT_APP_VERSION=1.0.0

# Debug Settings
REACT_APP_LOG_LEVEL=error
REACT_APP_SHOW_REDUX_DEVTOOLS=false