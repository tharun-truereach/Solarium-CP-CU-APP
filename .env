# Default environment configuration for Solarium Web Portal
# Copy this file to .env.local for local development overrides

# Environment identifier
REACT_APP_ENVIRONMENT=DEVELOPMENT

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3000

# Session Configuration (in minutes)
REACT_APP_SESSION_TIMEOUT_MIN=60

# Application Configuration
REACT_APP_APP_NAME=Solarium Web Portal
REACT_APP_APP_VERSION=1.0.0

# Feature Flags (for future use)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true

# Build Configuration
GENERATE_SOURCEMAP=true